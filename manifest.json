{
    "manifest_version": 3,
    "name": "FindMyBook",
    "version": "0.0.1",
    "description": "Filter search, to find pdf version of books.",
    "action": {
        "default_popup": "index.html"
      },
    "background": {
        "service_worker": "./scripts/service-worker.js",
        "type": "module"
      },
    "permissions": ["activeTab"],
    "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+F",
        "mac": "Command+F"
      }
    }
  }
  }